@model WeatherApp.Models.LoginDto
@*
*@
<!doctype html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Login</title>
</head>

<body>

    <div class="form-outline">
        <input type="text" required asp-for="Email" class="form-control mb-3" placeholder="E-Mail" />
        <input type="password" required asp-for="Password" class="form-control mb-3" placeholder="Password" />
        <button asp-controller="AccountController" asp-action="Logged" class="btn btn-outline-primary mb-3">
            Login   
        </button>
        
    </div>
    
</body>
